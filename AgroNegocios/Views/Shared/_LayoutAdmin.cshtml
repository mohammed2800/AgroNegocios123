    <!-- mCustomScrollbar JS
        ============================================ -->
    <script src="~/Scripts/admin/jquery.mCustomScrollbar.concat.min.js"></script>
    <script src="~/Scripts/admin/mCustomScrollbar-active.js"></script>
    <!-- metisMenu JS
        ============================================ -->
    <script src="~/Scripts/admin/metisMenu.min.js"></script>
    <script src="~/Scripts/admin/metisMenu-active.js"></script>

    <!-- main JS
        ============================================ -->
    <script src="~/Scripts/admin/main.js"></script>
