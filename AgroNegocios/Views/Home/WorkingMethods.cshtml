@using AgroNegocios.Resources

@{
    ViewBag.Title = "WorkingMethods";
}

<!-- ##### Breadcrumb Area Start ##### -->
<div class="breadcrumb-area bg-img bg-overlay jarallax" style="background-image: url('../../img/bg-img/18.jpg');">
    <div class="container h-100">
        <div class="row h-100 align-items-center">
            <div class="col-12">
                <div class="breadcrumb-text">
                    <h2>@HomeRes.WorkingMethods</h2>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="famie-breadcrumb">
    <div class="container">
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="@Url.Action("Index","Home")"><i class="fa fa-home"></i> @HomeRes.Home</a></li>
                <li class="breadcrumb-item active" aria-current="page">@HomeRes.WorkingMethods</li>
            </ol>
        </nav>
    </div>
</div>
<!-- ##### Breadcrumb Area End ##### -->

<!-- ##### About Us Area Start ##### -->
<section class="about-us-area">
    <div class="container">
        <div class="row align-items-center">

            <!-- About Us Thumbnail -->
            <div class="col-12 col-lg-6">
                <div class="about-us-thumbnail mb-30">
                    <img src="~/img/workingMethods.png" alt="">
                </div>
            </div>

            <!-- About Us Content -->
            <div class="col-12 col-lg-6">
                <div class="about-us-content mb-100">
                    <!-- Section Heading -->
                    <div class="section-heading">
                        <h2><span>@HomeRes.WorkingMethods</span></h2>
                        <img src="~/img/core-img/decor.png" alt="">
                    </div>
                    <p>
                        AGRO-NEGOCIOS holds a holistic approach for each project based on cost-effective analysis, increasing efficiency by focusing on the result to BOT a complete design process.
                        Our proven track record in deferent projects worldwide allow us to generate "out of the box" creative solutions for each challenge.
                    </p>
                </div>
            </div>

        </div>
    </div>
</section>
<!-- ##### About Us Area End ##### -->

<!-- ##### Farming Practice Area Start ##### -->
<section class="farming-practice-area bg-gray section-padding-100-50">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <!-- Section Heading -->
                <div class="section-heading text-center">
                    <h2><span>@HomeRes.WorkingMethods</span></h2>
                    <img src="~/img/core-img/decor2.png" alt="">
                </div>
            </div>
        </div>

        <div class="row">

            <!-- Single Farming Practice Area -->
            <div class="col-12 col-sm-6 col-lg-4">
                <div class="single-farming-practice-area mb-50 wow fadeInUp" data-wow-delay="100ms">
                    <!-- Thumbnail -->
                    <div class="farming-practice-thumbnail">
                        <img src="~/img/Technology.jpg" alt="" style="width:350px;height:250px">
                    </div>
                    <!-- Content -->
                    <div class="farming-practice-content text-center">
                        <!-- Icon -->
                        <div class="farming-icon">
                            <img src="~/img/core-img/sprout.png" alt="">
                        </div>
                        <h4>@HomeRes.Technology</h4>
                        <p>@HomeRes.TechnologyDesc</p>
                    </div>
                </div>
            </div>

            <!-- Single Farming Practice Area -->
            <div class="col-12 col-sm-6 col-lg-4">
                <div class="single-farming-practice-area mb-50 wow fadeInUp" data-wow-delay="300ms">
                    <!-- Thumbnail -->
                    <div class="farming-practice-thumbnail">
                        <img src="~/img/instruction.png" alt="" style="width:350px;height:250px">
                    </div>
                    <!-- Content -->
                    <div class="farming-practice-content text-center">
                        <!-- Icon -->
                        <div class="farming-icon">
                            <img src="~/img/core-img/sprout.png" alt="">
                        </div>
                        <h4>@HomeRes.Instructions</h4>
                        <p>@HomeRes.InstructionsDesc</p>
                    </div>
                </div>
            </div>

            <!-- Single Farming Practice Area -->
            <div class="col-12 col-sm-6 col-lg-4">
                <div class="single-farming-practice-area mb-50 wow fadeInUp" data-wow-delay="500ms">
                    <!-- Thumbnail -->
                    <div class="farming-practice-thumbnail">
                        <img src="~/img/Integration.png" alt="" style="width:350px;height:250px">
                    </div>
                    <!-- Content -->
                    <div class="farming-practice-content text-center">
                        <!-- Icon -->
                        <div class="farming-icon">
                            <img src="~/img/core-img/sprout.png" alt="">
                        </div>
                        <h4>@HomeRes.Integration</h4>
                        <p>@HomeRes.IntegrationDesc</p>
                    </div>
                </div>
            </div>

            <!-- Single Farming Practice Area -->
            <div class="col-12 col-sm-6 col-lg-4">
                <div class="single-farming-practice-area mb-50 wow fadeInUp" data-wow-delay="300ms">
                    <!-- Thumbnail -->
                    <div class="farming-practice-thumbnail">
                        <img src="~/img/get-support.jpg" alt="" style="width:350px;height:250px">
                    </div>
                    <!-- Content -->
                    <div class="farming-practice-content text-center">
                        <!-- Icon -->
                        <div class="farming-icon">
                            <img src="~/img/core-img/sprout.png" alt="">
                        </div>
                        <h4>@HomeRes.FinancialSupport</h4>
                        <p>@HomeRes.FinancialSupportDesc</p>
                    </div>
                </div>
            </div>

            <!-- Single Farming Practice Area -->
            <div class="col-12 col-sm-6 col-lg-4">
                <div class="single-farming-practice-area mb-50 wow fadeInUp" data-wow-delay="100ms">
                    <!-- Thumbnail -->
                    <div class="farming-practice-thumbnail">
                        <img src="~/img/implementation.png" alt="" style="width:350px;height:250px">
                    </div>
                    <!-- Content -->
                    <div class="farming-practice-content text-center">
                        <!-- Icon -->
                        <div class="farming-icon">
                            <img src="~/img/core-img/sprout.png" alt="">
                        </div>
                        <h4>@HomeRes.ImplementationAndSupport</h4>
                        <p>@HomeRes.ImplementationAndSupportDesc</p>
                    </div>
                </div>
            </div>

            <!-- Single Farming Practice Area -->
            <div class="col-12 col-sm-6 col-lg-4">
                <div class="single-farming-practice-area mb-50 wow fadeInUp" data-wow-delay="500ms">
                    <!-- Thumbnail -->
                    <div class="farming-practice-thumbnail">
                        <img src="~/img/managmentpng.png" alt="" style="width:350px;height:250px">
                    </div>
                    <!-- Content -->
                    <div class="farming-practice-content text-center">
                        <!-- Icon -->
                        <div class="farming-icon">
                            <img src="~/img/core-img/sprout.png" alt="">
                        </div>
                        <h4>@HomeRes.Management</h4>
                        <p>@HomeRes.ManagementDesc</p>
                    </div>
                </div>
            </div>

        </div>

    </div>
</section>
<!-- ##### Farming Practice Area End ##### -->

