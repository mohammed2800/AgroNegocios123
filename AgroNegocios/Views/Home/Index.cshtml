<!-- ##### About Us Area Start ##### -->
<section class="about-us-area">
    <div class="container">
        <div class="row align-items-center">

            <!-- About Us Content -->
            <div class="col-12 col-md-8">
                <div class="about-us-content mb-100">
                    <!-- Section Heading -->
                    <div class="section-heading">
                        <p>@HomeRes.AboutUs</p>
                        <h2><span>@HomeRes.LetUs</span> @HomeRes.TellYouOurStory</h2>
                        <img src="~/img/core-img/decor.png" alt="">
                    </div>
                    <p>
                        @HomeRes.AgroStory
                    </p>
                    <a href="@Url.Action("About","Home")" class="famie-btn mt-30">@HomeRes.ReadMore</a>
                </div>
            </div>

            <!-- Famie Video Play -->
            <div class="col-12 col-md-4">
                <div class="famie-video-play mb-100">
                    <img src="~/img/bg-img/6.jpg" alt="">
                    <!-- Play Icon -->
                    <a href="@HomeRes.Video" class="play-icon"><i class="fa fa-play"></i></a>
                </div>
            </div>

        </div>
    </div>
</section>
